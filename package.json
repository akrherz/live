  "type": "module",
  "scripts": {
    "test": "node --test tests/lsr-parser.test.mjs tests/layer-config.test.mjs tests/date-utils.test.mjs",
    "test:coverage": "mkdir -p coverage && node --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stderr --test-reporter=lcov --test-reporter-destination=coverage/lcov.info tests/lsr-parser.test.mjs tests/layer-config.test.mjs tests/date-utils.test.mjs",
    "test:lsr": "node --test tests/lsr-parser.test.mjs",
    "test:layer-config": "node --test tests/layer-config.test.mjs",
    "test:date-utils": "node --test tests/date-utils.test.mjs",
    "sync:extjs": "node scripts/sync-extjs-assets.mjs",
    "prestart": "npm run sync:extjs",
    "predev": "npm run sync:extjs",
    "prebuild": "npm run sync:extjs",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "extjs-gpl": "^6.2.0",
    "globals": "^17.0.0",
    "iemjs": "^0.0.7",
    "ol": "^10.7.0",
    "strophe.js": "^4.0.0-rc0"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "vite": "^7.3.1"
  }
